{
  "name": "react-expressjs",
  "version": "4.0.0",
  "description": "Boilerplate for ReactJS project with ExpressJS server",
  "main": "server/bin/server.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack/client.dev.js",
    "build": "yarn run build:client && yarn run build:server",
    "prod": "cross-env NODE_ENV=production webpack --config webpack/client.prod.js --progress",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack/server.prod.js --progress",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack/client.ssr.prod.js --progress",
    "start": "cd server && node bin/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tahnik/react-express-webpack-babel"
  },
  "keywords": [
    "react",
    "express",
    "webpack",
    "babel",
    "expressjs",
    "react router v4",
    "redux"
  ],
  "author": "Tahnik Mustasin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tahnik/react-express-webpack-babel/issues"
  },
  "homepage": "https://github.com/tahnik/react-express-webpack-babel#readme",
  "dependencies": {
    "@types/react": "^16.4.17",
    "@types/react-dom": "^16.0.9",
    "@types/react-redux": "^6.0.9",
    "@types/react-router": "^4.0.32",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux": "^3.6.0",
    "axios": "^0.18.0",
    "cross-env": "^5.1.4",
    "ejs": "^2.5.9",
    "express": "^4.16.3",
    "npm": "^6.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.1.2",
    "react-markdown": "^4.0.3",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-rte": "^0.16.1",
    "react-spinners": "^0.4.7",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@types/draft-js": "^0.10.25",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "bootstrap": "3.4.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "3.2.1",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.0",
    "redux-saga": "^0.16.2",
    "sass-loader": "^7.0.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.21.0",
    "typescript": "^3.1.3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "0.5.9",
    "webpack": "4.6.0",
    "webpack-cli": "2.0.15",
    "webpack-dev-server": "^3.1.3",
